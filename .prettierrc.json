{
  "stylelintIntegration": true,
  "eslintIntegration": true,
  "semi": true,
  "tabWidth": 2,
  "useTabs": false,
  "bracketSpacing": true,
  "printWidth ": 100,
  "singleQuote": true,
  "proseWrap": "never",
  "trailingComma": "all",
  "arrowParens": "always",
  "htmlWhitespaceSensitivity": "ignore",
  "printWidth": 100,
  "endOfLine": "auto",
  "ignorePath": ".prettierignore",
  "requireConfig": false
}
